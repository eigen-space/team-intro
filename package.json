{
    "name": "@eigenspace/team-intro",
    "private": false,
    "version": "0.1.0",
    "keywords": [
        "eigenspace",
        "development"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "dev": "gatsby develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "lint:code": "eslint --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "yarn lint:code --fix",
        "lint:markdown": "node ./node_modules/@eigenspace/codestyle/scripts/markdown-lint.js",
        "lint": "yarn lint:code . && yarn lint:markdown",
        "hooks:pre-push": "yarn lint"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn hooks:pre-push",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn lint:fix",
            "git add"
        ]
    },
    "dependencies": {
        "react": "16.13.1",
        "react-dom": "16.13.1"
    },
    "devDependencies": {
        "@eigenspace/codestyle": "latest",
        "@eigenspace/commit-linter": "0.0.21",
        "@hot-loader/react-dom": "16.13.0",
        "@types/prop-types": "15.7.3",
        "@types/react": "16.9.35",
        "eslint-plugin-eigenspace-script": "latest",
        "eslint-plugin-prettier": "3.1.2",
        "gatsby": "2.21.22",
        "gatsby-plugin-sass": "2.3.1",
        "gatsby-plugin-sharp": "2.6.2",
        "gatsby-source-filesystem": "2.3.8",
        "gatsby-transformer-remark": "2.8.13",
        "gatsby-transformer-sharp": "2.5.2",
        "husky": "1.3.1",
        "lint-staged": "8.1.5",
        "node-sass": "4.14.1",
        "prettier": "2.0.5",
        "typescript": "3.5.3"
    }
}
